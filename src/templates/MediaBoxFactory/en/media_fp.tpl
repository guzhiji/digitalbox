<!-- flash ("swf") -->
<object width="{$Width}" height="{$Height}">
    <param name="movie" value="{$Address}"></param>
    <param name="allowFullScreen" value="true"></param>
    <param name="allowscriptaccess" value="always"></param>
    <embed src="{$Address}" type="application/x-shockwave-flash" width="{$Width}" height="{$Height}" allowscriptaccess="always" allowfullscreen="true"></embed>
</object>